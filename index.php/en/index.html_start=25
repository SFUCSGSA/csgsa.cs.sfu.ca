	<h2>
					<a href="/index.php/en/113-cs-gsa-bike-ride">
			CS GSA Bike Ride!</a>
			</h2>

	<ul class="actions">
				<li class="print-icon">
			<a href="/index.php/en/113-cs-gsa-bike-ride?tmpl=component&amp;print=1&amp;layout=default&amp;page=" title="Print" onclick="window.open(this.href,'win2','status=no,toolbar=no,scrollbars=yes,titlebar=no,menubar=no,resizable=yes,width=640,height=480,directories=no,location=no'); return false;" rel="nofollow"><img src="/media/system/images/printButton.png" alt="Print"  /></a>		</li>
